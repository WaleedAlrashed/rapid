{
  "name": "rapid-ui",
  "version": "0.0.1",
  "description": "Rapid JavaScript development environment for building UI",
  "private": true,
  "scripts": {
    "xpreserve": "run-p eslint test",
    "serve": "webpack-dev-server --config bin/webpack.config.dev.babel.js --color",
    "xprebuild": "npm run clean",
    "build": "cross-env NODE_ENV=production webpack --config bin/webpack.config.prod.babel.js --color",
    "eslint": "eslint bin/**/*.js --color",
    "replace!stylint": "stylint ui --color",
    "replace!stylint:watch": "chokidar --initial --silent \"ui/**/*.styl\" -c \"npm run stylint\"",
    "test": "mocha --reporter progress bin/test.js ui/**/*.test.js --color",
    "test:watch": "chokidar --initial --silent \"ui/**/*.test.js\" -c \"npm run test\"",
    "clean": "rimraf dist",
    "share": "run-p application localtunnel",
    "localtunnel": "lt --port 3001 --subdomain rapid-ui",
    "new!deploy": "npm run build && <some build script>"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nuotron/rapid-ui.git"
  },
  "keywords": [
    "ui",
    "starter-kit",
    "boilerplate",
    "webpack",
    "rapid"
  ],
  "author": "Nuotron <hello@nuotron.co>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nuotron/rapid-ui/issues"
  },
  "homepage": "https://github.com/nuotron/rapid-ui#readme",
  "dependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/register": "^7.0.0",
    "babel-loader": "^8.0.4",
    "browser-sync": "^2.26.3",
    "browser-sync-webpack-plugin": "^2.2.2",
    "chai": "^4.2.0",
    "chalk": "^2.4.1",
    "chokidar-cli": "^1.2.1",
    "copy-webpack-plugin": "^5.0.3",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.1",
    "eslint": "^5.8.0",
    "eslint-config-rapid": "^0.1.0",
    "eslint-loader": "^2.1.2",
    "extract-loader": "^3.1.0",
    "file-loader": "^2.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jsdom": "^13.0.0",
    "kouto-swiss": "^1.1.0",
    "localtunnel": "^1.9.1",
    "lodash": "^4.17.11",
    "mini-css-extract-plugin": "^0.5.0",
    "mocha": "^5.2.0",
    "npm-run-all": "^4.1.3",
    "postcss-loader": "^3.0.0",
    "pug": "^2.0.3",
    "pug-html-loader": "^1.1.5",
    "pug-lint": "^2.5.0",
    "pug-lint-config-rapid": "^0.1.0",
    "pug-lint-loader": "0.0.4",
    "rapid-error-handler": "0.0.1",
    "rimraf": "^2.6.2",
    "style-loader": "^0.23.1",
    "stylelint": "^10.0.1",
    "stylelint-webpack-plugin": "^0.10.5",
    "stylint": "^1.5.9",
    "stylint-loader": "^1.0.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "webpack": "^4.25.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-merge": "^4.1.4"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": "> 0.25%, not dead"
        }
      ]
    ]
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "browser": true,
      "es6": true,
      "node": true,
      "mocha": true
    },
    "extends": "rapid",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    }
  },
  "pugLintConfig": {
    "extends": "rapid"
  },
  "stylintrc": {
    "blocks": false,
    "brackets": "never",
    "colons": "always",
    "colors": "always",
    "commaSpace": "always",
    "commentSpace": "always",
    "cssLiteral": "never",
    "customProperties": [],
    "depthLimit": false,
    "duplicates": true,
    "efficient": "always",
    "exclude": [],
    "extendPref": "@extend",
    "globalDupe": true,
    "groupOutputByFile": true,
    "indentPref": 2,
    "leadingZero": "never",
    "maxErrors": false,
    "maxWarnings": false,
    "mixed": true,
    "mixins": [],
    "namingConvention": "camelCase",
    "namingConventionStrict": true,
    "none": "always",
    "noImportant": true,
    "parenSpace": "never",
    "placeholders": "never",
    "prefixVarsWithDollar": "never",
    "quotePref": "single",
    "reporterOptions": {
      "columns": [
        "lineData",
        "severity",
        "description",
        "rule"
      ],
      "columnSplitter": "  ",
      "showHeaders": false,
      "truncate": true
    },
    "semicolons": "never",
    "sortOrder": "grouped",
    "stackedProperties": "never",
    "trailingWhitespace": "never",
    "universal": false,
    "valid": true,
    "zeroUnits": "never",
    "zIndexNormalize": false
  }
}
