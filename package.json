{
  "name": "rapid-response",
  "version": "0.1.0",
  "description": "Error handling made easier",
  "main": "index.js",
  "author": "Nuotron <hi@nuotron.co>",
  "license": "ISC",
  "scripts": {
    "lint": "eslint src/*.js --color"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.4.4",
    "eslint": "^5.16.0",
    "eslint-config-rapid": "git+https://YahiaRefaiea@bitbucket.org/nuotron/eslint-config-rapid.git"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": "> 0.25%, not dead"
        }
      ]
    ]
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "browser": true,
      "es6": true,
      "node": true,
      "mocha": true
    },
    "extends": "rapid",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    }
